@using System.IO
@using System.Text
@using EstruturaPDF.Helpers

@{
    // Caminho para o diretório que contém os SVGs
    var base64Svgs = PdfHelper.ConvertAllSvgsToBase64();
    var verifica = false;
    var data = DateTime.Now;
    string? texto = null;
    Dictionary<string, string> leito = new Dictionary<string, string>
{
{ "normal", "Leito Normal" },
{ "vermelho", "Leito Vermelho" },
{ "fino", "Leito Fino"}
};
    Dictionary<string, string> comprimentoUnha = new Dictionary<string, string>
{
{ "pequeno", "Pequeno" },
{ "medio", "Médio" },
{ "grande", "Grande"}
};
    Dictionary<string, string> tipoUnha = new Dictionary<string, string>
{
{ "plana", "Plana" },
{ "roida", "Roída" },
{ "concava", "Côncava"},
{ "convexa", "Convexa"}
};
    Dictionary<string, string> tipoMao = new Dictionary<string, string>
{
{ "normal", "Normal" },
{ "seca", "Seca" },
{ "mista", "Mista"},
{ "oleosa", "Oleosa"}
};

    var base64String =
    "";
    var imageSource = $"data:image/png;base64,{base64String}";
}

<!DOCTYPE html>
<html>

<head>
    <style>
        body {
            margin: 0;
            /* Remova margens externas */
            padding: 20px;
            /* Adicione padding se necessário */
            width: 100%;
            /* Preencha a largura total */
            height: auto;
            /* Altura automática para o conteúdo */
            box-sizing: border-box;
            /* Inclui padding e bordas na largura total */
            color: #4e4e4e
        }

        h2 {
            font-size: 18px;
            align-items: center;
            justify-content: center;
            display: flex;
        }

        h3 {
            font-size: 17px;
            text-align: left;
            font-weight: 400;
            margin-bottom: 12px;
            margin-right: auto;
            margin-left: 6px;
            margin-top: revert;

        }

        div {
            display: flex;
            align-items: center;
            justify-content: center;
            flex-direction: column;
        }

        table {
            caption-side: bottom;
            border-collapse: collapse;
            width: 100%;
        }

        th,
        td {
            /* Borda cinza clara */
            text-align: center;
            /* Alinhamento à esquerda */
            padding: 3px;
            /* Espaçamento interno */
            width: 50%;
            /* Cada coluna ocupa 50% da tabela */
            border-radius: 8px;
            box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.085);
        }

        .td-custom {
            text-align: left;
        }

        .warning-message {
            color: rgb(53, 52, 52);
            font-size: 15px;
            display: flex;
            align-items: center; // Alinha verticalmente o texto
            margin-right: 2px; // Espaçamento entre o texto e o formulário, ajuste conforme necessário
            flex: 0 1 auto;
            word-wrap: break-word;
            /* Quebra palavras longas para evitar overflow */
            overflow-wrap: break-word;
            /* Garante a quebra de palavras em navegadores modernos */
            hyphens: auto;
            /* Hifeniza palavras longas automaticamente */
            box-sizing: border-box;
            /* Inclui padding e border na largura total do box */
            max-width: 100%;/
        }

        .container-custom {
            border-radius: 13px;
            padding: 7px;
            box-shadow: 0px 0px 1px #ededed;
            width: 98%;
            text-align: left !important;
        }

        .avoid-break {
            page-break-inside: avoid;
            width: 100%;
        }

        .break-page {
            page-break-before: always;
        }

        .title-message {
            font-size: 16px;
            text-align: left;
            margin-right: auto;
            color: #414040
        }

        .img-custom {
            display: flex;
            align-items: center;
            margin-right: auto;
        }

        .text-message {
            font-size: 15px;
            display: flex;
            align-items: center; // Alinha verticalmente o texto
            margin-right: 2px; // Espaçamento entre o texto e o formulário, ajuste conforme necessário
            flex: 0 1 auto;
            word-wrap: break-word;
            /* Quebra palavras longas para evitar overflow */
            overflow-wrap: break-word;
            /* Garante a quebra de palavras em navegadores modernos */
            hyphens: auto;
            /* Hifeniza palavras longas automaticamente */
            box-sizing: border-box;
            /* Inclui padding e border na largura total do box */
            max-width: 100%;/
        }

        hr {
            border: 1px solid #ededed;
            margin: 19px 0;
            width: -webkit-fill-available;
        }

        .img-reduced {
            height: 100px;
            /* Ajuste para a altura desejada */
            width: auto;
            /* Mantém a proporção da imagem */
        }
    </style>
</head>

<body>
    <div style='text-align: center;'>
        <img style='width:80px; height:80px;' src='data:image/svg+xml;base64,@base64Svgs["icon"]' alt='Ícone'/>
        <br>
    </div>
    <div>
        <h2>Ficha Anamnese</h2>
        <h3>Histórico Médico</h3>
        <div class="section avoid-break">
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Gestante?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(true, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Diabetes?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Possui algum tipo de alergia?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Problemas na tireoide?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Bariátrica?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Possui psoríase?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Algum outro problema que seja necessário nos informar?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Se sim, especifique</td>
                        <td class="td-custom">@Html.Raw(PdfHelper.PreencherCampoVazio(texto))</td>
                    </tr>
                </tbody>
            </table>
            <br>
            <h3>Histórico de Traumas e Lesões</h3>
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Já teve ou tem unhas das mãos encravadas?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Como que está seu leito ungueal?</td>
                        <td>@Html.Raw(PdfHelper.RederRadioGruop(leito, base64Svgs, "vermelho"))</td>
                    </tr>
                </tbody>
            </table>
            <br>
            <br>
            <div>
                <span class="warning-message">
                    Traumas ou lesões nas unhas podem afetar a durabilidade de alongamentos e tratamentos. Se você
                    teve
                    problemas com a manutenção de tratamentos, isso pode ser um fator a ser considerado.
                    <br>
                    Obs: unhas oleosas e unhas com deficiências de saúde tendem a comprometer a durabilidade do
                    alongamento.
                </span>
            </div>

            <br>
            <h3>Histórico de Problemas nas Unhas</h3>
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Onicocriptose (unha encravada)?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Onicofagia (Roer as unhas)?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Já teve algum problema de onicomicose (micose, fungos ou outros)?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Se sim, especifique</td>
                        <td class="td-custom">@Html.Raw(PdfHelper.PreencherCampoVazio(texto))</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="section avoid-break">
            <h3>Avaliação das Condições Ungueais</h3>
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Descamações na lâmina ungueal?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Estrias na lâmina ungueal?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Manchas na lâmina ungueal?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Descolamento da lâmina ungueal?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                </tbody>
            </table>
            <h3>Hábitos e Exposições</h3>
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Pratica esportes de impacto?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Frequenta piscina ou mar diariamente?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                </tbody>
            </table>
            <h3>Cuidados e Rotina</h3>
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Já fez a remoção de alongamento em casa?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Suas unhas naturais já foram compridas?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Vai na manicure frequentemente?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Já utilizou alongamento antes?</td>
                        <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Porque deseja utilizar alongamento:</td>
                        <td class="td-custom">@Html.Raw(PdfHelper.PreencherCampoVazio(texto))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Qual o tipo de alongamento e por quanto tempo, especifique:</td>
                        <td class="td-custom">@Html.Raw(PdfHelper.PreencherCampoVazio(texto))</td>
                    </tr>
                </tbody>
            </table>
            <h3>Tipos de Unhas e Mãos</h3>
            <table>
                <thead>
                    <tr>
                        <th>Pergunta</th>
                        <th>Resposta Fornecida</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="td-custom">Comprimento das Unhas:</td>
                        <td>@Html.Raw(PdfHelper.RederRadioGruop(comprimentoUnha, base64Svgs, "medio"))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Tipo de Pele das Mãos:</td>
                        <td>@Html.Raw(PdfHelper.RederRadioGruop(tipoMao, base64Svgs, "seca"))</td>
                    </tr>
                    <tr>
                        <td class="td-custom">Forma Predominante das Unhas:</td>
                        <td>@Html.Raw(PdfHelper.RederRadioGruop(tipoUnha, base64Svgs, "roida"))</td>
                    </tr>
                </tbody>
            </table>
            <br>
            <br>
            <div>
                <span class="warning-message">
                    Atenção: Em alguns casos, unhas roídas podem impedir a realização do procedimento de manutenção,
                    sendo
                    necessário realizar uma nova aplicação. Neste caso, será cobrado o valor integral de uma nova
                    aplicação.
                    Além disso, não há garantia de que o resultado final das unhas roídas fique 100% satisfatório, pois
                    o
                    leito
                    da unha costuma ser muito curto.
                </span>
            </div>
        </div>
        <br>
        <div class="section avoid-break">
            <h3>Documentação de Termos e Observações</h3>
            <div>
                <span class="title-message">
                    <br>
                    Manutenção das Unhas Alongadas:
                </span>
                <span class="warning-message">
                    ● Este processo requer cuidados regulares, com manutenções que devem ser realizadas dentro do prazo
                    estipulado. Os intervalos para manutenção podem variar entre 10, 15, 20 e 25 dias, de acordo com os
                    cuidados
                    que você tem com suas unhas e o crescimento natural delas. A frequência da manutenção será
                    determinada
                    pela
                    Nail Designer responsável.
                </span>
                <span class="title-message">
                    <br>
                    Quebra do Alongamento:
                </span>
                <span class="warning-message">
                    ● O alongamento de unhas, embora mais resistente do que as unhas naturais, pode sim quebrar. Caso
                    isso
                    ocorra, as unhas quebradas podem ser restauradas pelo profissional.
                </span>
                <span class="title-message">
                    <br>
                    Durabilidade do Alongamento:
                </span>
                <span class="warning-message">
                    ● A durabilidade do seu alongamento é influenciada em 50% pela aplicação do profissional (técnica
                    correta e
                    uso dos melhores produtos do mercado) e 50% pelos cuidados que você tem com as suas unhas, qualidade
                    das
                    suas unhas naturais e o cumprimento dos prazos de manutenção.
                </span>
                <span class="warning-message">
                    <br>
                    Observação: Unhas quebradas ou caídas não têm garantia. Para manutenção dessas unhas, será cobrada
                    uma
                    taxa
                    de serviço (R$ _ por unha).
                </span>
            </div>
            <br>
            <div>
                <span class="title-message">
                    Autorização para Registro Fotográfico:
                </span>
                <span class="warning-message">
                    Autorizo o registro fotográfico do trabalho realizado (“antes” e “depois”) para fins de
                    documentação e
                    divulgação em livros, redes sociais ou qualquer material publicitário. A autorização é concedida
                    gratuitamente, sem que eu tenha a reclamar qualquer crédito ou outra compensação.
                </span>
                <br>
                <span class="img-custom">
                    @Html.Raw(PdfHelper.RenderCheckBox(true, base64Svgs))
                </span>
            </div>
            <br>
            <div class="container-custom">
                <table>
                    <thead>
                        <tr>
                            <th>Pergunta</th>
                            <th>Resposta Fornecida</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="td-custom">Menor de idade?</td>
                            <td>@Html.Raw(PdfHelper.RenderRadioButtonIcons(verifica, base64Svgs))</td>
                        </tr>
                        <tr>
                            <td class="td-custom">Nome do responsável</td>
                            <td class="td-custom">@Html.Raw(PdfHelper.PreencherCampoVazio("José Aldo"))</td>
                        </tr>
                        <tr>
                            <td class="td-custom">RG do responsável</td>
                            <td class="td-custom">@Html.Raw(PdfHelper.PreencherCampoVazio(texto))</td>
                        </tr>
                    </tbody>
                </table>
                <span class="warning-message">
                    <br>
                    Eu, @Html.Raw(PdfHelper.PreencherCampoVazio(texto)), do RG
                    @Html.Raw(PdfHelper.PreencherCampoVazio(texto)),
                    autorizo minha (meu) filha(o) a fazer o procedimento de
                    alongamento de unhas.
                </span>
            </div>
            <br>
            <div class="text-message">
                <span class="title-message">
                    Declaração de Veracidade:
                </span>
                <span class="warning-message">
                    Afirmo que estou sendo verdadeira ao preencher essa ficha, e me responsabilizo caso ocorra algo que
                    seja
                    prejudicial por ter omitido informações.
                </span>
                <hr>
                <img class="img-reduced" src="@imageSource" alt="Descrição da imagem" />
                <br>
                <span>Data Assinatura:</span>
                <span>@Html.Raw(PdfHelper.FormatarData(data))</span>
                <hr>
            </div>
            <table>
                <thead>
                    <tr>
                        <th>Data Atendimento</th>
                        <th>Data Manutenção recomendada</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Html.Raw(PdfHelper.FormatarData(data))</td>
                        <td>@Html.Raw(PdfHelper.FormatarData(data))</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>
<a href="@Url.Action("GeneratePdf", "Pdf")" class="btn btn-primary">Gerar PDF</a>
